<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>PrimeiroTeste</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>servletTeste</servlet-name>
  	<servlet-class>servlets.PrimeiroServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>servletCadastro</servlet-name>
  	<servlet-class>servlets.ServletCadastro</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>servletTeste</servlet-name>
  	<url-pattern>/teste</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>servletCadastro</servlet-name>
  	<url-pattern>/cadastrar</url-pattern>
  </servlet-mapping>
  
</web-app>